{
  "manifest_version": 2,
  "name": "云盘钥匙",
  "description": "调取别人接口，制作云盘钥匙",
  "version": "1.0",
  "browser_action": {
    "default_title": "请勿用于商业用途，欢迎赞助。",
    "default_popup": "hello.html"
  },
  "icons": {
    "19": "img/icon_19.png",
    "38": "img/icon_38.png",
    "128": "img/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://pan.baidu.com/share/init*",
        "http://pan.baidu.com/share/init*",
        "https://pan.baidu.com/wap/init*",
        "http://pan.baidu.com/wap/init*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "get_code.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "exclude_matches": [
        "https://pan.baidu.com/share/init*",
        "http://pan.baidu.com/share/init*",
        "*://*.bilibili.com/*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "findAndReplaceDOMText.js",
        "test_link.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "*://ypsuperkey.meek.com.cn/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}