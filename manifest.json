{
  "manifest_version": 2,
  "name": "test",
  "description": "测试",
  "version": "0.1",
  "browser_action": {
    "default_popup": "hello.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://pan.baidu.com/share/init*",
        "http://pan.baidu.com/share/init*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "get_code.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "exclude_matches": [
        "https://pan.baidu.com/share/init*",
        "http://pan.baidu.com/share/init*",
        "https://*.bilibili.com/*",
        "https://*.csdn.net/*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "test_link.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "*://ypsuperkey.meek.com.cn/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}